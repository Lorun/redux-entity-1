{
  "name": "redux-entity",
  "version": "5.1.0",
  "description": "A predictable approach to managing domain entities in Redux.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clear && npm run compile",
    "compile": "babel src -d lib",
    "test": "nyc mocha --require babel-core/register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clear": "rm -rf ./lib",
    "example": "node example.js"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "temp-directory": "./.nyc-coverage-temp",
    "report-dir": "./.nyc-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/redux-entity.git"
  },
  "keywords": [
    "redux",
    "redux-thunk",
    "asynchronous"
  ],
  "author": "Mike Chabot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikechabot/redux-entity/issues"
  },
  "homepage": "https://github.com/mikechabot/redux-entity#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "expect": "^1.20.2",
    "mocha": "^5.1.1",
    "nyc": "^12.0.2",
    "redux": "^4.0.0",
    "redux-mock-store": "~1.5.3",
    "redux-thunk": "^2.3.0"
  },
  "tonicExampleFilename": "example.js"
}
